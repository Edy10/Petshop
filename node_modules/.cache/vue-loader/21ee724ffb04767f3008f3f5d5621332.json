/>\n      </div>\n      <div class=\"espaco-abaixo\">\n        <label>Serviço:</label>\n        <select>\n          <option v-for=\"(servico, index) in servicos\" :key=\"index\">{{servico.nome}} ({{servico.preco | grana}})</option>\n        </select>\n      </div>\n      <button>Cadastrar Cliente</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  data(){\n    return{\n      racas: [],\n      servicos: []\n    }\n  },\n  async created(){\n    this.racas = await this.buscarRacas();\n    this.servicos = await this.buscaServicos();\n  },\n  methods: {\n    async buscarRacas() {\n      const {data} = await axios.get(\"http://localhost:3000/racas\");\n      return data;\n    },\n    async buscaServicos() {\n      const {data} = await axios.get(\"http://localhost:3000/servicos\");\n      return data;\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  label{\n    margin-right: 10px;\n  }\n</style>"]}]}